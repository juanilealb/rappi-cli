# Base site for automation (Argentina production site)
RAPPI_BASE_URL=https://www.rappi.com.ar

# Session state generated by `rappi-cli login bootstrap`
RAPPI_SESSION_FILE=/home/your-user/.config/rappi-cli/session-state.json

# Browser settings
RAPPI_HEADLESS=false
RAPPI_SLOWMO_MS=0

# Default city hint used in restaurants search URL query
RAPPI_DEFAULT_CITY=Buenos Aires
